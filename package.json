{
  "name": "exif-renamer",
  "version": "0.1.1",
  "type": "module",
  "scripts": {
    "build:server": "tsc --project ./tsconfig-server.json",
    "build:web": "vite build",
    "build:webdev": "vite",
    "build:app": "concurrently \"npm run build:server\" \"npm run build:web\"",
    "build:dev": "concurrently \"npm run build:server -- --watch\" \"npm run build:web -- --watch\"",
    "build:preview": "vite preview",
    "build": "npm run build:app",
    "svg:icons": "spritely -i src/ui/0_assets/svg/icons -q -o src/ui/0_assets/svg/inline/icons.svg",
    "svg:optimize": "svgo -f ./src/ui/0_assets/svg -r --config=.svgo.yml",
    "svg": "npm run svg:optimize && npm run svg:icons",
    "storybook": "start-storybook -p 9002",
    "lint": "eslint .",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "start": "electron ./backend/server.js",
    "test": "jest"
  },
  "dependencies": {
    "@emotion/css": "^11.7.1",
    "@storybook/addon-svelte-csf": "^1.1.0",
    "classnames": "^2.3.1",
    "color": "^4.1.0",
    "lodash": "^4.17.21",
    "svelte": "^3.45.0",
    "vanillajs-browser-helpers": "^2.0.2",
    "vanillajs-helpers": "^3.1.3"
  },
  "noInstallDependencies": {
    "date-fns": "^2.16.1",
    "file-loader": "^6.2.0",
    "fs-promise": "^2.0.3",
    "md5-file": "^5.0.0",
    "node-exiftool": "^2.3.0"
  },
  "devDependencies": {
    "@microflash/spritely": "^1.4.0",
    "@storybook/addon-essentials": "^6.4.10",
    "@storybook/svelte": "^6.4.10",
    "@sveltejs/vite-plugin-svelte": "^1.0.0-next.34",
    "@tsconfig/node16": "^1.0.2",
    "@tsconfig/svelte": "^3.0.0",
    "@types/classnames": "^2.3.1",
    "@types/color": "^3.0.2",
    "@types/jest": "^27.4.0",
    "@types/lodash": "^4.14.178",
    "@types/node": "^17.0.8",
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^5.9.0",
    "concurrently": "^7.0.0",
    "cross-env": "^7.0.3",
    "electron": "^16.0.6",
    "eslint": "^8.6.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-html": "^6.2.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-svelte3": "^3.3.0",
    "exiftool-vendored": "^15.9.2",
    "file-loader": "^6.2.0",
    "globby": "^12.0.2",
    "jest": "^27.4.7",
    "module-alias": "^2.2.2",
    "storybook": "^6.4.9",
    "storybook-builder-vite": "^0.1.13",
    "svelte-check": "^2.2.11",
    "svelte-loader": "^3.1.2",
    "svelte-preprocess": "^4.10.1",
    "svg-inline-loader": "^0.8.2",
    "svgo": "^2.8.0",
    "ts-jest": "^27.1.2",
    "ts-loader": "^9.2.6",
    "tslib": "^2.3.1",
    "typescript": "^4.5.4",
    "vite": "^2.7.10"
  }
}
