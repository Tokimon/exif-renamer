{
  "name": "exif-renamer",
  "version": "0.1.1",
  "scripts": {
    "build:server": "tsc --project ./tsconfig-server.json",
    "build:web": "webpack -c ./webpack.config.js",
    "build:app": "concurrently \"npm run build:server\" \"npm run build:web\"",
    "build:dev": "concurrently \"npm run build:server -- --watch\" \"npm run build:web -- --watch\"",
    "build": "npm run build:app && npm run svg:sprite",
    "start": "electron ./main/server.js",
    "lint": "eslint .",
    "svg:sprite": "spritely -i src/ui/svg/icons -q -o renderer/icons.svg",
    "svgo": "svgo -f ./src/ui/svg -r --config=.svgo.yml",
    "storybook": "start-storybook"
  },
  "_moduleAliases": {
    "~": "main/",
    "vanillajs-helpers/ts": "node_modules/vanillajs-helpers/cjs"
  },
  "dependencies": {
    "@emotion/css": "^11.1.3",
    "classnames": "^2.2.6",
    "color": "^3.1.3",
    "date-fns": "^2.16.1",
    "lodash": "^4.17.20",
    "svelte": "^3.31.0",
    "vanillajs-browser-helpers": "^1.1.8",
    "vanillajs-helpers": "^2.0.2"
  },
  "optionalDependencies": {
    "date-fns": "^2.16.1",
    "exiftool-vendored": "^13.0.0",
    "file-loader": "^6.2.0",
    "fs-promise": "^2.0.3",
    "md5-file": "^5.0.0",
    "node-exiftool": "^2.3.0"
  },
  "devDependencies": {
    "@microflash/spritely": "^1.3.1",
    "@storybook/addon-actions": "^6.1.11",
    "@storybook/svelte": "^6.1.11",
    "@tsconfig/node14": "^1.0.0",
    "@tsconfig/svelte": "^1.0.10",
    "@types/classnames": "^2.2.11",
    "@types/color": "^3.0.1",
    "@types/lodash": "^4.14.166",
    "@types/node": "^14.14.16",
    "@typescript-eslint/eslint-plugin": "^4.11.1",
    "@typescript-eslint/parser": "^4.11.1",
    "concurrently": "^5.3.0",
    "cross-env": "^7.0.3",
    "electron": "^11.1.1",
    "eslint": "^7.16.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-html": "^6.1.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-svelte3": "^3.0.0",
    "file-loader": "^6.2.0",
    "globby": "^11.0.1",
    "html-webpack-plugin": "^4.5.0",
    "module-alias": "^2.2.2",
    "storybook": "^6.1.11",
    "svelte-loader": "^2.13.6",
    "svelte-preprocess": "^4.6.1",
    "svgo": "^1.3.2",
    "ts-loader": "^8.0.12",
    "tslib": "^2.0.3",
    "webpack": "^5.11.1",
    "webpack-cli": "^4.3.0"
  }
}