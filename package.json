{
  "name": "exif-renamer",
  "version": "0.1.1",
  "scripts": {
    "build:server": "tsc --project ./tsconfig-server.json",
    "build:web": "webpack -c ./webpack.config.js",
    "build:app": "concurrently \"npm run build:server\" \"npm run build:web\"",
    "build:dev": "concurrently \"npm run build:server -- --watch\" \"npm run build:web -- --watch\"",
    "build": "npm run build:app",
    "svg:icons": "spritely -i src/ui/0_assets/svg/icons -q -o src/ui/0_assets/svg/inline/icons.svg",
    "svg:optimize": "svgo -f ./src/ui/svg -r --config=.svgo.yml",
    "svg": "npm run svg:optimize && npm run svg:icons",
    "storybook": "start-storybook -s .storybook/public",
    "lint": "eslint .",
    "start": "electron ./main/server.js"
  },
  "_moduleAliases": {
    "~": "main/",
    "vanillajs-helpers/ts": "node_modules/vanillajs-helpers/cjs"
  },
  "dependencies": {
    "@emotion/css": "^11.1.3",
    "classnames": "^2.2.6",
    "color": "^3.1.3",
    "lodash": "^4.17.20",
    "svelte": "^3.31.2",
    "vanillajs-browser-helpers": "^1.1.8",
    "vanillajs-helpers": "^2.0.2"
  },
  "noInstallDependencies": {
    "date-fns": "^2.16.1",
    "file-loader": "^6.2.0",
    "fs-promise": "^2.0.3",
    "md5-file": "^5.0.0",
    "node-exiftool": "^2.3.0"
  },
  "devDependencies": {
    "@microflash/spritely": "^1.3.1",
    "@storybook/addon-actions": "^6.2.0-alpha.10",
    "@storybook/addon-backgrounds": "^6.2.0-alpha.10",
    "@storybook/addon-controls": "^6.2.0-alpha.10",
    "@storybook/addon-docs": "^6.2.0-alpha.10",
    "@storybook/svelte": "^6.2.0-alpha.10",
    "@tsconfig/node14": "^1.0.0",
    "@tsconfig/svelte": "^1.0.10",
    "@types/classnames": "^2.2.11",
    "@types/color": "^3.0.1",
    "@types/lodash": "^4.14.167",
    "@types/node": "^14.14.20",
    "@typescript-eslint/eslint-plugin": "^4.12.0",
    "@typescript-eslint/parser": "^4.12.0",
    "concurrently": "^5.3.0",
    "copy-webpack-plugin": "^7.0.0",
    "cross-env": "^7.0.3",
    "electron": "^11.1.1",
    "eslint": "^7.17.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-html": "^6.1.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-svelte3": "^3.0.0",
    "exiftool-vendored": "^13.0.0",
    "file-loader": "^6.2.0",
    "globby": "^11.0.2",
    "html-webpack-plugin": "^5.0.0-beta.1",
    "module-alias": "^2.2.2",
    "storybook": "^6.1.11",
    "svelte-loader": "^2.13.6",
    "svelte-preprocess": "^4.6.1",
    "svg-inline-loader": "^0.8.2",
    "svgo": "^1.3.2",
    "ts-loader": "^8.0.14",
    "tslib": "^2.1.0",
    "webpack": "^5.12.2",
    "webpack-cli": "^4.3.1"
  }
}