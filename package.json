{
  "name": "exif-renamer",
  "version": "0.1.1",
  "type": "module",
  "scripts": {
    "build:server": "tsc --project ./tsconfig-server.json",
    "build:web": "vite build",
    "build:webdev": "vite",
    "build:app": "concurrently \"npm run build:server\" \"npm run build:web\"",
    "build:dev": "concurrently \"npm run build:server -- --watch\" \"npm run build:web -- --watch\"",
    "build:preview": "vite preview",
    "build": "npm run build:app",
    "svg:icons": "spritely -i src/ui/0_assets/svg/icons -q -o src/ui/0_assets/svg/inline/icons.svg",
    "svg:optimize": "svgo -f ./src/ui/0_assets/svg -r --config=.svgo.yml",
    "svg": "npm run svg:optimize && npm run svg:icons",
    "storybook": "storybook dev -p 9000",
    "lint": "eslint .",
    "prettier": "prettier --write .",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "start": "electron ./backend/server.js",
    "test": "jest",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@jsfns/core": "^1.1.1",
    "@jsfns/web": "^1.1.1",
    "color": "^4.2.3",
    "lodash": "^4.17.21",
    "svelte": "^4.2.2"
  },
  "noInstallDependencies": {
    "date-fns": "^2.16.1",
    "file-loader": "^6.2.0",
    "fs-promise": "^2.0.3",
    "md5-file": "^5.0.0",
    "node-exiftool": "^2.3.0"
  },
  "devDependencies": {
    "@microflash/spritely": "^1.4.0",
    "@storybook/addon-essentials": "^7.5.2",
    "@storybook/addon-interactions": "7.5.2",
    "@storybook/addon-links": "7.5.2",
    "@storybook/addon-svelte-csf": "^4.0.9",
    "@storybook/blocks": "7.5.2",
    "@storybook/builder-vite": "^7.5.2",
    "@storybook/svelte": "^7.5.2",
    "@storybook/svelte-vite": "7.5.2",
    "@storybook/testing-library": "0.2.2",
    "@sveltejs/vite-plugin-svelte": "^2.4.6",
    "@trivago/prettier-plugin-sort-imports": "^4.2.1",
    "@tsconfig/node16": "^16.1.1",
    "@tsconfig/svelte": "^5.0.2",
    "@types/color": "^3.0.5",
    "@types/jest": "^29.5.7",
    "@types/lodash": "^4.14.200",
    "@types/node": "^20.8.10",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "electron": "^27.0.3",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-html": "^7.1.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jest": "^27.6.0",
    "eslint-plugin-n": "^16.3.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-storybook": "^0.6.15",
    "eslint-plugin-svelte": "^2.34.1",
    "exiftool-vendored": "^23.4.0",
    "globby": "^13.2.2",
    "jest": "^29.7.0",
    "module-alias": "^2.2.3",
    "prettier": "^3.0.3",
    "prettier-plugin-svelte": "^3.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^7.5.2",
    "svelte-check": "^3.5.2",
    "svelte-eslint-parser": "^0.33.1",
    "svgo": "^3.0.2",
    "ts-jest": "^29.1.1",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2",
    "vite": "^4.5.0",
    "vite-tsconfig-paths": "^4.2.1"
  },
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  }
}
